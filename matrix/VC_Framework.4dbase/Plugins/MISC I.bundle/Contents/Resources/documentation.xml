<?xml version="1.0" encoding="UTF-8" standalone="no" ?><ex4D:plugin xmlns:ex4D="http://www.4d.com/plugin">  <!--MISC I-->  <ex4D:documentation name="MISC I" plugin-id="20000">    <ex4D:plugin-description>      <!-- write documentation for MISC I here -->	  <p>A collection of (hopefully) useful tools.</p>    </ex4D:plugin-description>    <ex4D:commands>      <ex4D:command-group command-group-id="1" name="Picture">        <ex4D:command command-id="1" name="PICTURE GET RAW DATA">          <ex4D:command-description>            <!-- write documentation for PICTURE GET RAW DATA here -->			<p>Retrives the raw picture data and format description from a given 4D picture.</p>			          </ex4D:command-description>          <ex4D:argument argument-id="1" label="picture" type="P">            <!-- write documentation for $1 (PICTURE) here-->			<p>The 4D picture to examine.</p>			          </ex4D:argument>          <ex4D:argument argument-id="2" label="formats" type="Y">            <!-- write documentation for $2 (ARRAY) here-->			<p>Array of text that decribe the picture format, i.e. the extensions, MIME types and OS types.</p>			          </ex4D:argument>          <ex4D:argument argument-id="3" label="pictures" type="Y">            <!-- write documentation for $3 (ARRAY) here-->			<p>Array of pictures in their native formats.</p>			          </ex4D:argument>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="2" name="Stack">        <ex4D:command command-id="2" name="STACK Get available size">          <ex4D:command-description>            <!-- write documentation for STACK Get available size here -->			<p>This function returns the amount of stack available.</p>			          </ex4D:command-description>          <ex4D:return-value label="size" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>The amount of free stack.</p>			          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="3" name="Process">        <ex4D:command command-id="3" name="PROCESS GET LIST">          <ex4D:command-description>            <!-- write documentation for PROCESS GET LIST here -->			<p>The receiving 3 arrays must be declared appropriately prior to call. On Windows the <a href="http://msdn.microsoft.com/en-us/library/ms682489(VS.85).aspx">CreateToolhelp32Snapshot</a> function is used. On Mac, the <a href="http://developer.apple.com/library/mac/#documentation/Cocoa/Reference/ApplicationKit/Classes/NSWorkspace_Class/Reference/Reference.html%23//apple_ref/doc/uid/TP40004153">NSWorkspace</a> class is used.</p>			          </ex4D:command-description>          <ex4D:argument argument-id="1" label="names" type="Y">            <!-- write documentation for $1 (ARRAY) here-->			<p>Process names.</p>			          </ex4D:argument>          <ex4D:argument argument-id="2" label="paths" type="Y">            <!-- write documentation for $2 (ARRAY) here-->			<p>Process paths.</p>			          </ex4D:argument>          <ex4D:argument argument-id="3" label="numbers" type="Y">            <!-- write documentation for $3 (ARRAY) here-->			<p>Process numbers.</p>			          </ex4D:argument>        </ex4D:command>        <ex4D:command command-id="4" name="PROCESS Get id">          <ex4D:command-description>            <!-- write documentation for PROCESS Get id here -->          </ex4D:command-description>          <ex4D:return-value label="process" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>The process number of the current application.</p>          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="4" name="System">        <ex4D:command command-id="5" name="SYSTEM Get timestamp">          <ex4D:command-description>            <!-- write documentation for SYSTEM Get timestamp here -->			<p>This command returns the current time in 2010-10-23T02:53:00Z format.</p>			          </ex4D:command-description>          <ex4D:return-value label="timestamp" type="T">            <!-- write documentation for return value (TEXT) here-->          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="6" name="SYSTEM Get timestring">          <ex4D:command-description>            <!-- write documentation for SYSTEM Get timestring here -->			<p>This command returns the current time in Sat, 23 Oct 10 02:53:00 GMT format.</p>			          </ex4D:command-description>          <ex4D:return-value label="timestring" type="T">            <!-- write documentation for return value (TEXT) here-->          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="7" name="SYSTEM Generate UUID">          <ex4D:command-description>            <!-- write documentation for SYSTEM Generate UUID here -->			<p>This command generates a UUID in 7B01F15A-2222-4708-A7DA-2670E498454B format. Internally the function <a href="http://developer.apple.com/library/mac/#DOCUMENTATION/CoreFoundation/Reference/CFUUIDRef/Reference/reference.html">CFUUIDCreate</a> is used on Mac and <a href="http://msdn.microsoft.com/en-us/library/aa379205(VS.85).aspx">UuidCreate</a> on Windows. The value is also converted to uppercase on Windows to unify the format.</p>			          </ex4D:command-description>          <ex4D:return-value label="uuid" type="T">            <!-- write documentation for return value (TEXT) here-->          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="8" name="SYSTEM Get unixtime">          <ex4D:command-description>            <!-- write documentation for SYSTEM Get unixtime here -->			<p>This command returns the current time in 1287802380 format.</p>			          </ex4D:command-description>          <ex4D:return-value label="unixtime" type="T">            <!-- write documentation for return value (TEXT) here-->          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="5" name="Structure">        <ex4D:command command-id="9" name="STRUCTURE Import definition">          <ex4D:command-description>            <!-- write documentation for STRUCTURE Import definition here -->			<p>This command call the plugin entry point to add tables, fields, relations from an XML definition.</p>          </ex4D:command-description>          <ex4D:argument argument-id="1" label="xml" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>The structure defintion. You can create one from the Structure Edtitor.</p>          </ex4D:argument>          <ex4D:return-value label="error" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>Error code.</p>          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="6" name="Volume">        <ex4D:command command-id="10" name="VOLUME Is remote">          <ex4D:command-description>            <!-- write documentation for VOLUME Is remote here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="volume" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>The volume name, as returned from the VOLUME LIST command.</p>			<p>Alternatively, a file path or folder path.</p>								          </ex4D:argument>          <ex4D:argument argument-id="2" label="remote" type="L">            <!-- write documentation for $2 (LONGINT) here-->		  <p>1, if remote.</p>			          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>1, on success.</p>			          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="11" name="VOLUME Is ejectable">          <ex4D:command-description>            <!-- write documentation for VOLUME Is ejectable here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="volume" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>The volume name, as returned from the VOLUME LIST command.</p>			<p>Alternatively, a file path or folder path.</p>			          </ex4D:argument>          <ex4D:argument argument-id="2" label="ejectable" type="L">            <!-- write documentation for $2 (LONGINT) here-->			<p>1, if ejectable.</p>			          </ex4D:argument>          <ex4D:return-value label="success" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>1, on success.</p>			          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="7" name="Get Path">        <ex4D:command command-id="12" name="PATH Get known folder">          <ex4D:command-description>            <!-- write documentation for PATH Get known folder here -->			<p>This commmand returns the file path of specific directories on the system. It can be useful, for example, to find out the actual path to My Documents on Windows, which maybe moved to a non standard location.</p>			          </ex4D:command-description>          <ex4D:argument argument-id="1" label="type" type="L">            <!-- write documentation for $1 (LONGINT) here-->			<p>The selector to specify which directory you want to search.</p>			          </ex4D:argument>          <ex4D:return-value label="path" type="T">            <!-- write documentation for return value (TEXT) here-->			<p>The file path.</p>			          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="13" name="PATH From user selection">          <ex4D:command-description>            <!-- write documentation for PATH From user selection here -->			<p>This command presents the standard save file dialog, without actually creating any file.</p>			          </ex4D:command-description>          <ex4D:argument argument-id="1" label="file" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>The initial file name to present in the dialog.</p>			          </ex4D:argument>          <ex4D:argument argument-id="2" label="folder" type="T">            <!-- write documentation for $2 (TEXT) here-->			<p>The directory path of the initial location to present.</p>			          </ex4D:argument>          <ex4D:argument argument-id="3" label="title" type="T">            <!-- write documentation for $3 (TEXT) here-->			<p>The message to show on the window title.</p>			          </ex4D:argument>          <ex4D:return-value label="path" type="T">            <!-- write documentation for return value (TEXT) here-->			<p>The file path.</p>			          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>    </ex4D:commands>    <ex4D:constants>      <ex4D:constant-group constant-group-id="1" name="Known Folder Path" number="20000">        <ex4D:constant constant-id="1" name="WIN My Documents" type="L" value="0">          <!-- write documentation for WIN My Documents (0) here-->        </ex4D:constant>        <ex4D:constant constant-id="2" name="WIN App Data Local" type="L" value="1">          <!-- write documentation for WIN App Data Local (1) here-->        </ex4D:constant>        <ex4D:constant constant-id="3" name="WIN App Data Common" type="L" value="2">          <!-- write documentation for WIN App Data Common (2) here-->        </ex4D:constant>        <ex4D:constant constant-id="4" name="WIN App Data Roaming" type="L" value="3">          <!-- write documentation for WIN App Data Roaming (3) here-->        </ex4D:constant>        <ex4D:constant constant-id="5" name="MAC Documents" type="L" value="0">          <!-- write documentation for MAC Documents (0) here-->        </ex4D:constant>        <ex4D:constant constant-id="6" name="MAC App Support User" type="L" value="1">          <!-- write documentation for MAC App Support User (1) here-->        </ex4D:constant>        <ex4D:constant constant-id="7" name="MAC App Support Local" type="L" value="2">          <!-- write documentation for MAC App Support Local (2) here-->        </ex4D:constant>      </ex4D:constant-group>    </ex4D:constants>  </ex4D:documentation></ex4D:plugin>